<PaperCard as |card|>
  <PaperToolbar>
    <PaperToolbarTools>
      <h2>Currently Reading</h2>
    </PaperToolbarTools>
  </PaperToolbar>
  <PaperContent>
    <PaperList>
      {{#each @readings as |reading|}}
        <Reading::Multiple::Row
          @reading={{reading}}
          @onComplete={{@onComplete}}
        />
      {{/each}}
    </PaperList>
  </PaperContent>
</PaperCard>
